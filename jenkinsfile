pipeline {
    agent any
    stages {
        stage("Checkout") {
            steps {
            git url: 'https://github.com/amitkumar1211/springboot-gradle-example.git'
            }
        }
        stage("Compile") {
            steps {
            bat "./gradlew compileJava"
            }
        }
        stage("Unit test") {
            steps {
            bat "./gradlew test"
            }
        }
    }
}
